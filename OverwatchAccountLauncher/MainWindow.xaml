<Window x:Class="OverwatchAccountLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OverwatchAccountLauncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Create User Files" HorizontalAlignment="Left" Height="97" Margin="48,88,0,0" VerticalAlignment="Top" Width="194" Click="Button_Click_1"/>
        <Button x:Name="ButtonOne" Content="No Account" HorizontalAlignment="Left" Height="97" Margin="501,150,0,0" VerticalAlignment="Top" Width="207" Click="Button_Click_2"/>
        <TextBlock x:Name="UserAccount" HorizontalAlignment="Left" Height="21" Margin="225,47,0,0" TextWrapping="Wrap" Text="No User" VerticalAlignment="Top" Width="58"/>
        <Button Content="Load User Profiles" HorizontalAlignment="Left" Height="97" Margin="48,203,0,0" VerticalAlignment="Top" Width="194" Click="Button_Click_4"/>
    </Grid>
</Window>
